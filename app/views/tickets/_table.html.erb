<div class="ticket_table">
  <p><%= link_to 'New Ticket', new_ticket_url %></p>
  
  <table cellspacing="0" border="0" width="100%" class="standard">
    <thead>
      <tr>
        <th>#</th>
        <th>Ticket</th>
        <th>Milestone</th>
        <th>Age</th>
        <th>Priority</th>
        <th>Stage</th>
        <th>Owner</th>
      </tr>
    </thead>
    <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= link_to ticket.id, edit_ticket_url(ticket) %>
        <td><%= link_to ticket.title, edit_ticket_url(ticket) %></td>
        <td><%=h ticket.milestone.name if ticket.milestone %></td>
        <td>1 day old</td>
        <td><%=h ticket.priority.name if ticket.priority %></td>
        <td><%=h ticket.stage.name if ticket.stage %></td>
        <td><%=h ticket.owner.name if ticket.owner %></td>
    <% end %>
    </tbody>
  </table>
</div>